{
    "name": "calculator-app",
    "version": "1.0.0",
    "description": "Kalkulator w Node.js z interfejsem graficznym, build, start i test",
    "type": "module",
    "main": "dist/app.js",
    "scripts": {
        "build": "babel src -d dist",
        "start": "node dist/app.js",
        "dev": "nodemon --exec babel-node src/app.js",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "express": "^4.18.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.21.0",
        "@babel/core": "^7.21.0",
        "@babel/node": "^7.21.0",
        "@babel/preset-env": "^7.21.0",
        "babel-jest": "^29.7.0",
        "jest": "^29.0.0",
        "nodemon": "^2.0.22"
    }
}
